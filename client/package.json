{
  "name": "todo-hypermedia-client",
  "version": "0.0.1",
  "author": "toddb",
  "description": "Sample clients to demonstrate the use of hypermedia",
  "productName": "Todo client",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./build",
    "dev": "webpack-dev-server --mode development --open --config webpack.dev.js --hot",
    "test": "jest --config=jest.config.js",
    "test-watch": "jest --watch --config=jest.config.js",
    "test-watch-notify": "jest --watch --notify --config=jest.config.js",
    "test-clear-cache": "jest --clearCache",
    "coverage": "jest --coverage --config=jest.config.js",
    "build": "webpack --mode development --config webpack.dev.js --progress --hide-modules",
    "dist-api": "webpack --mode production --config webpack.prod.api.js --colors false --progress false",
    "dist-app": "webpack --mode production --config webpack.prod.app.js --colors false --progress false",
    "publish-api-demo": "yarn run dist-api && aws s3 sync ./dist/api/ s3://cdn.semanticlink.io",
    "publish-app-demo": "yarn run dist-app && aws s3 sync ./dist/app/ s3://todo.semanticlink.io"
  },
  "dependencies": {
    "auth-header": "^1.0.0",
    "auth0-js": "9.10.2",
    "axios": "^0.18.0",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.20",
    "bottleneck": "^2.7.1",
    "fast-json-patch": "2.1.0",
    "linkifyjs": "^2.1.6",
    "luxon": "1.14.0",
    "semantic-link": "^0.1.1",
    "underscore": "^1.9.1",
    "vue": "2.6.10",
    "vue-datetime": "^1.0.0-beta.6",
    "vue-ionicons": "^2.2.1",
    "vue-localstorage": "^0.6.2",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.6",
    "vue-simple-spinner": "^1.2.8",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/jest": "24.0.13",
    "@types/jest-each": "0.3.4",
    "@types/mocha": "5.2.6",
    "@types/underscore": "1.8.18",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "7.13.0",
    "favicons-webpack-plugin": "^0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-each": "^23.5.0",
    "node-sass": "^4.9.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "string-replace-webpack-plugin": "^0.1.3",
    "ts-jest": "^23.1.3",
    "ts-loader": "^5.3.0",
    "typescript": "^3.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">=8.9.1"
  }
}
